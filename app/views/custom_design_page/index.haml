.container-fluid
  #clothe-tshirt-maker

:javascript

  loadjs(["/assets/frontend/custom/image_maker/jquery-ui.min.js", "/assets/frontend/custom/image_maker/jquery.ui.touch-punch.min.js", "/assets/frontend/custom/image_maker/imageMaker.min.js", "/assets/frontend/custom/image_maker/imageMaker.min.css", "/assets/frontend/lib/slick.min.js", "/assets/frontend/lib/slick.css", "/assets/frontend/lib/slick-theme.css"], 'custom_design');
  loadjs.ready('custom_design', function() {

    var template_t_shirt = [{url: "#{image_url('design/t-shirt-white.png')}", title:'HUHU'}, {url: "#{image_url('design/t-shirt-black.png')}", title:'Gundam Head Skate'}]
    
    $('#clothe-tshirt-maker').imageMaker({
      merge_images:[
        {url: "#{image_url('design/gundam-head-skate.png')}", title:'Gundam Head Skate'},
        {url: "#{image_url('design/wolf-head-skate.png')}", title:'Wolf Head Skate'},
        {url: "#{image_url('design/gorilla-head-skate.png')}", title:'Gorilla Head Skate'},
        {url: "#{image_url('design/sasuke-chibi.png')}", title:'Sasuke Chibi'},
        {url: "#{image_url('design/gundam-head-skate.png')}", title:'Gundam Head Skate'},
        {url: "#{image_url('design/pain-head.png')}", title:'Pain Head'},
        {url: "#{image_url('design/wolf-head-skate.png')}", title:'Wolf Head Skate'}
      ],
      templates: template_t_shirt,
      text_boxes_count:0,
      preRender:function(html){ html+="<div>this is my alter</div>"; return html;},
      merge_image_thumbnail_width:15,
      template_thumbnail_width: '100%',
      template_thumbnail_height: 'auto',
      merge_image_thumbnail_width: '100%',
      merge_image_thumbnail_height: 'auto',
      downloadGeneratedImage: false,
      onGenerate: function(data, formData){          
        $.ajax({
        type: "POST",
        processData: false,
        contentType: false,
        url: "#{home_custom_design_path}",
        data: formData,
        success: function (data) {},
        complete: function() {}})
      },
    });

    $('.single-item').not('.slick-initialized').slick({
      autoplay:true,
      dots: true,
      infinite: true
    });

    $('.image-maker-template-image').not('.slick-initialized').slick({
      slidesToShow: 4,
      slidesToScroll: 4,
      autoplay:true,
      dots: true,
      infinite: true
    });

    $('.image-maker-thumbnail-image').not('.slick-initialized').slick({
      slidesToShow: 5,
      slidesToScroll: 1,
      autoplay:false,
      dots: true,
      infinite: true
    });
  })